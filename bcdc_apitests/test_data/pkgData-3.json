    "object_name": "WHSE_WASTE.SITE_BOREHOLE_LITHOLOGY_SP",
    "state": "active",
    "version": null,
    "details": [
        {
            "data_precision": "10",
            "column_comments": "BOREHOLE LITHOLOGY ID  is a system generated unique identification number. This is a primary key. e.g., 70",
            "data_type": "NUMBER",
            "short_name": "BRHL_ID",
            "column_name": "BOREHOLE_LITHOLOGY_ID"
        },
        {
            "data_precision": "5",
            "column_comments": "BOREHOLE SITE ID is the site ID which is assigned to each remediated site. e.g., 7010",
            "data_type": "NUMBER",
            "short_name": "BRHL_S_ID",
            "column_name": "BOREHOLE_SITE_ID"
        },
        {
            "data_precision": "255",
            "column_comments": "BOREHOLE NAME is the borehole name assigned by the driller of the borehole. e.g., MW2002-2",
            "data_type": "VARCHAR2",
            "short_name": "BRHL_NAME",
            "column_name": "BOREHOLE_NAME"
        },
        {
            "data_precision": "255",
            "column_comments": "BOREHOLE ADDRESS is the address at which the borehole was drilled. e.g., 952 Granville Street",
            "data_type": "VARCHAR2",
            "short_name": "BRHL_ADDR",
            "column_name": "BOREHOLE_ADDRESS"
        },
        {
            "data_precision": "100",
            "column_comments": "BOREHOLE MUNICIPALITY is the municipality in which the borehole was drilled. e.g., Langley",
            "data_type": "VARCHAR2",
            "short_name": "BRHL_MUN",
            "column_name": "BOREHOLE_MUNICIPALITY"
        },
        {
            "data_precision": "8",
            "column_comments": "LATITUDE is the latitude of the site where the borehole was drilled, in decimal degrees. e.g., 49.280249",
            "data_type": "NUMBER",
            "short_name": "LATITUDE",
            "column_name": "LATITUDE"
        },
        {
            "data_precision": "9",
            "column_comments": "LONGITUDE is the longitude of the site where the borehole was drilled, in decimal degrees. e.g., -123.119281",
            "data_type": "NUMBER",
            "short_name": "LONGITUDE",
            "column_name": "LONGITUDE"
        },
        {
            "data_precision": "7",
            "column_comments": "DATE DRILLED is the date on which the borehole was drilled. e.g., 2007-05-24",
            "data_type": "DATE",
            "short_name": "DATE_DRILL",
            "column_name": "DATE_DRILLED"
        },
        {
            "data_precision": "5",
            "column_comments": "TOTAL DEPTH is the total depth of the borehole, in meters. e.g., 6.1",
            "data_type": "NUMBER",
            "short_name": "TOTAL_DPTH",
            "column_name": "TOTAL_DEPTH"
        },
        {
            "data_precision": "5",
            "column_comments": "TOC ELEVATION is the elevation of the top of the casing, in meters above sea level. e.g., 25.5",
            "data_type": "NUMBER",
            "short_name": "TOC_ELEV",
            "column_name": "TOC_ELEVATION"
        },
        {
            "data_precision": "5",
            "column_comments": "SURFACE ELEVATION is the ground surface elevation, in meters above sea level. e.g., 25.6",
            "data_type": "NUMBER",
            "short_name": "SURF_ELEV",
            "column_name": "SURFACE_ELEVATION"
        },
        {
            "data_precision": "5",
            "column_comments": "DEPTH TO TOP OF SCREEN is where the top of the well screen is located, in meters below ground surface. e.g., 4.5",
            "data_type": "NUMBER",
            "short_name": "DPTH_TO_TP",
            "column_name": "DEPTH_TO_TOP_OF_SCREEN"
        },
        {
            "data_precision": "5",
            "column_comments": "DEPTH TO BOTTOM OF SCREEN is where the bottom of the well screen is located, in meters below ground surface. e.g., 6.7",
            "data_type": "NUMBER",
            "short_name": "DPTH_TO_BT",
            "column_name": "DEPTH_TO_BOTTOM_OF_SCREEN"
        },
        {
            "data_precision": "5",
            "column_comments": "FROM UNIT DEPTH is where the lithological unit begins, in meters. e.g., 2.6",
            "data_type": "NUMBER",
            "short_name": "F_UNIT_DPT",
            "column_name": "FROM_UNIT_DEPTH"
        },
        {
            "data_precision": "5",
            "column_comments": "TO UNIT DEPTH is where the lithological unit ends, in meters. e.g., 3.7",
            "data_type": "NUMBER",
            "short_name": "T_UNIT_DPT",
            "column_name": "TO_UNIT_DEPTH"
        },
        {
            "data_precision": "3",
            "column_comments": "UNIT NUMBER is the lithological unit number. e.g., 1",
            "data_type": "NUMBER",
            "short_name": "UNIT_NUM",
            "column_name": "UNIT_NUMBER"
        },
        {
            "data_precision": "255",
            "column_comments": "UNIT DESCRIPTION is the description of the lithological unit. e.g., Sand (fine to medium), silty, dark grey",
            "data_type": "VARCHAR2",
            "short_name": "UNIT_DESC",
            "column_name": "UNIT_DESCRIPTION"
        },
        {
            "data_precision": "12",
            "column_comments": "HYDRAULIC CONDUCTIVITY is the hydraulic conductivity in meters per second. e.g., 0.000000572",
            "data_type": "NUMBER",
            "short_name": "HYDRLC_CON",
            "column_name": "HYDRAULIC_CONDUCTIVITY"
        },
        {
            "data_precision": "5",
            "column_comments": "DEPTH TO WATER BELOW TOC  is the depth to water, in meters below the top of the casing. e.g., 3.8",
            "data_type": "NUMBER",
            "short_name": "D_T_WB_TOC",
            "column_name": "DEPTH_TO_WATER_BELOW_TOC"
        },
        {
            "data_precision": "5",
            "column_comments": "DEPTH TO WATER BELOW SURFACE is the depth to water, in meters below the ground surface. e.g., 4.6",
            "data_type": "NUMBER",
            "short_name": "D_T_WB_SUR",
            "column_name": "DEPTH_TO_WATER_BELOW_SURFACE"
        },
        {
            "data_precision": "500",
            "column_comments": "SUPPORTING DOCUMENT is a path and file name of scanned PDF version of the borehole log. Example of a file name is 11623_MW09-1.pdf",
            "data_type": "VARCHAR2",
            "short_name": "SUP_DOC",
            "column_name": "SUPPORTING_DOCUMENT"
        },
        {
            "data_precision": "1",
            "column_comments": "SHAPE is the column used to reference the spatial coordinates defining the feature.",
            "data_type": "SDO_GEOMETRY",
            "short_name": "SHAPE",
            "column_name": "SHAPE"
        },
        {
            "data_precision": "38",
            "column_comments": "OBJECTID is a required attribute of feature classes and object classes in a geodatabase",
            "data_type": "NUMBER",
            "short_name": "OBJECTID",
            "column_name": "OBJECTID"
        },
        {
            "data_precision": "4000",
            "column_comments": "SE ANNO CAD DATA is added by ArcGIS to support CAD and annotation features when the spatial type is SDO_GEOMETRY.",
            "data_type": "BLOB",
            "short_name": "SE_ANNO_CA",
            "column_name": "SE_ANNO_CAD_DATA"
        }
    ],
